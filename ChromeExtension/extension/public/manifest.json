{
  "name": "Love Reminder",
  "description": "Happy 3rd Anniversary ❤️ Love you Babe",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logoFavicon.jpeg.48x48.png",
    "48": "logoFavicon.jpeg.48x48.png",
    "128": "logoFavicon.jpeg.48x48.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ]
}